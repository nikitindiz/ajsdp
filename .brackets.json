{
    "path": {
        "bsearch/sass/*.sass": {
            "sass.enabled": true,
            "sass.options": {
                "includePaths": [
                    "../css/"
                ],
                "outputDir": "../css/",
                "imagePath": null,
                "sourceComments": "map",
                "outputStyle": "nested"
            }
        }
    }
}